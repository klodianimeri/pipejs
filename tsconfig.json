{
    "compilerOptions": {
        "target": "es2018",
        "module": "esnext",
        "moduleResolution": "node",
        "sourceMap": true,
        "rootDir": "src",
        "outDir": "dist",
        // "noEmit": false,
        // "baseUrl": ".",
        "removeComments": true,
        "downlevelIteration": true,
        "declaration": true
    },
    "exclude": [
        "node_modules",
        "*.test.ts",
        "**/**/**/*.test.ts",
        "dist/*.d.ts",
        "dist/**/*.d.ts"
    ]
}